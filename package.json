{
	"name": "interactive-cd",
	"version": "1.1.0",
	"private": true,
	"homepage": "https://github.com/bdfinst/interactive-cd#readme",
	"bugs": {
		"url": "https://github.com/bdfinst/interactive-cd/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bdfinst/interactive-cd.git"
	},
	"type": "module",
	"scripts": {
		"build": "npm run validate:data && vite build",
		"commit": "npx @commitlint/cli --help-url https://github.com/conventional-changelog/commitlint/#what-is-commitlint",
		"commit:validate": "npx commitlint --edit",
		"dev": "vite dev",
		"format:fix": "prettier --write '**/*.{js,yml,yaml,json,css,scss,toml,md,svelte}'",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepare": "husky",
		"preview": "vite preview",
		"start": "npm run dev",
		"test": "vitest --run",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:ui": "vitest --ui",
		"test:watch": "vitest --watch",
		"update": "npx npm-check-updates -u",
		"validate:data": "node scripts/validate-cd-practices.js"
	},
	"lint-staged": {
		"*.{js,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,yml,yaml,css,scss}": [
			"prettier --write"
		],
		"src/lib/data/cd-practices.json": [
			"prettier --write",
			"npm run validate:data"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-netlify": "^5.2.4",
		"@sveltejs/kit": "^2.47.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@typescript-eslint/eslint-plugin": "^8.46.1",
		"@typescript-eslint/parser": "^8.46.1",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.5",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"jsdom": "^27.0.1",
		"lint-staged": "^16.2.4",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.41.0",
		"svelte-eslint-parser": "^1.4.0",
		"tailwindcss": "^4.1.14",
		"vite": "^7.1.10",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^7.1.0",
		"@fortawesome/free-brands-svg-icons": "^7.1.0",
		"@fortawesome/free-solid-svg-icons": "^7.1.0",
		"svelte-fa": "^4.0.4"
	}
}
